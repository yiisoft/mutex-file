{
    "name": "yiisoft/mutex-file",
    "type": "library",
    "description": "Yii Mutex Library - File Driver",
    "keywords": [
        "yii",
        "mutex"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/mutex-file",
        "issues": "https://github.com/yiisoft/mutex-file/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/mutex": "^3.0@dev",
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Mutex\\File\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Mutex\\Tests\\": [
                "vendor/yiisoft/mutex/tests"
            ],
            "Yiisoft\\Mutex\\File\\Tests\\": [
                "tests",
                "vendor/yiisoft/mutex/tests"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": [
            "phpunit --colors --no-interaction"
        ]
    }
}
