{
    "name": "yiisoft/mutex-file",
    "type": "library",
    "description": "Yii Mutex Library - File Driver",
    "keywords": [
        "yii",
        "mutex"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/mutex-file",
        "issues": "https://github.com/yiisoft/mutex-file/issues",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "yiisoft/files": "^1.0",
        "yiisoft/mutex": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.9",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.9"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Mutex\\File\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Mutex\\File\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    }
}
